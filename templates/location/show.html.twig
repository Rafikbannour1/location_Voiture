{% extends 'base.html.twig' %}

{% block title %}Location{% endblock %}

{% block body %}
    <h1>Location</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ location.id }}</td>
            </tr>
            <tr>
                <th>Cin</th>
                <td>{{ location.cin }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ location.nom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ location.email }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ location.dateDebut ? location.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ location.dateFin ? location.dateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Matricule_voiture</th>
                <td>{{ location.MatriculeVoiture }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('location_index') }}">back to list</a>

    <a href="{{ path('location_edit', {'id': location.id}) }}">edit</a>

    {{ include('location/_delete_form.html.twig') }}
{% endblock %}
